<p id="notice"><%= notice %></p>

<h1>Realty Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @realty_types.each do |realty_type| %>
      <tr>
        <td><%= realty_type.name %></td>
        <td><%= link_to 'Show', realty_type %></td>
        <td><%= link_to 'Edit', edit_realty_type_path(realty_type) %></td>
        <td><%= link_to 'Destroy', realty_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Realty Type', new_realty_type_path %>
