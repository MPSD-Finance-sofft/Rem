<%= form_with(model: file_board, local: true) do |form| %>
  <% if file_board.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(file_board.errors.count, "error") %> prohibited this file_board from being saved:</h2>

      <ul>
      <% file_board.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :text %>
    <%= form.text_area :text %>
  </div>

  <div class="field">
    <%= form.label :file %>
    <%= form.file_field :file %>
  </div>

  <div class="field">
    <%= form.label :permission %>
    <%= form.number_field :permission %>
  </div>

  <div class="field">
    <%= form.label :end %>
    <%= form.date_select :end %>
  </div>

  <div class="field">
    <%= form.label :start %>
    <%= form.date_select :start %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
