<h1>Notifications</h1>

<table class="responsive nowrap table table-hover" id="dttb-notifications" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="text">Text</th>
      <th data-data="object">Object</th>
      <th data-data="object_id">Object</th>
      <th data-data="user_id">User</th>
      <th data-data="active">Active</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Notification', new_notification_path %>