<h1>Cooperations</h1>

<table class="responsive nowrap table table-hover" id="dttb-cooperations" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="agent_id">Agent</th>
      <th data-data="date_of_notice">Date of notice</th>
      <th data-data="day_count">Day count</th>
      <th data-data="type_of_notice_id">Type of notice</th>
      <th data-data="user_id">User</th>
      <th data-data="or_request_id">Or request</th>
      <th data-data="notice">Notice</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

