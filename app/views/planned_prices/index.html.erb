<h1>Planned Prices</h1>

<table class="responsive nowrap table table-hover" id="dttb-planned_prices" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="accord_id">Accord</th>
      <th data-data="user_id">User</th>
      <th data-data="advertising_price">Advertising price</th>
      <th data-data="market_price">Market price</th>
      <th data-data="estimated_selling_price">Estimated selling price</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Planned Price', new_planned_price_path %>